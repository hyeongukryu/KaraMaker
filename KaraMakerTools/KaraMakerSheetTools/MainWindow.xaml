<Window x:Class="KaraMakerSheetTools.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KaraMakerSheetTools"
        mc:Ignorable="d"
        Title="KaraMakerSheetTools" Height="350" Width="525">
    <DockPanel Margin="10">
        <StackPanel Margin="0,0,0,10" Height="25" Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Padding="15,0" Click="Validate" Margin="0,0,5,0">데이터 검증</Button>
            <Button Padding="15,0" Click="Import" Margin="5,0">불러오기</Button>
            <Button Padding="15,0" Click="Export" Margin="5,0">내보내기</Button>
        </StackPanel>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition />
            </Grid.RowDefinitions>
            <TextBox />
        </Grid>
    </DockPanel>
</Window>
